{
  "name": "json-schema-diff",
  "version": "0.3.0",
  "description": "A language agnostic CLI tool and nodejs api to identify differences between two json schema files.",
  "bin": {
    "json-schema-diff": "./bin/json-schema-diff"
  },
  "main": "dist/api.js",
  "scripts": {
    "release-major": "./node_modules/.bin/gulp release --type major",
    "release-minor": "./node_modules/.bin/gulp release --type minor",
    "release-patch": "./node_modules/.bin/gulp release --type patch",
    "test": "./node_modules/.bin/gulp",
    "watch": "./node_modules/.bin/gulp watch",
    "watch-e2e": "./node_modules/.bin/gulp watch-e2e"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org/atlassian/json-schema-diff.git"
  },
  "keywords": [
    "diff",
    "difference",
    "json",
    "schema",
    "json-schema"
  ],
  "author": {
    "name": "Ben Sayers (Atlassian)",
    "email": "bsayers@atlassian.com"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.14.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/lodash": "^4.14.108",
    "@types/node": "^9.6.5",
    "@types/verror": "^1.10.3",
    "conventional-changelog-lint": "^2.1.1",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.1.1",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.0.1",
    "gulp-conventional-changelog": "^1.1.24",
    "gulp-git": "^2.5.2",
    "gulp-jasmine": "^4.0.0",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^4.0.2",
    "lodash": "^4.17.10",
    "minimist": "^1.2.0",
    "run-sequence": "^2.2.1",
    "tslint": "5.9.1",
    "typescript": "2.8.3"
  },
  "types": "lib/api-types.d.ts",
  "dependencies": {
    "ajv": "^6.4.0",
    "commander": "^2.15.1",
    "verror": "^1.10.0"
  }
}
